{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "#account",
    "type": "object",
    "required": ["account"],
    "properties": {
        "account": {
            "type": "object",
            "required": [
                "homePage",
                "name"
            ],
            "properties": {
                "homePage": {
                    "type": "string",
                    "format": "iri"
                },
                "name": {"type": "string"}
            }
        }
    }
}
