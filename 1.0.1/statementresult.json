{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "#statementresult",
    "type": "object",
    "required": ["statements"],
    "properties": {
        "statements": {
            "type": "array",
            "items": {"$ref": "#statement"}
        },
        "more": {
            "type": "string",
            "format": "iri"
        }
    }
}
